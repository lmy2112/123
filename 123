#include <stdio.h>
int main( )
{                                 
  int max(int x,int y);                 
  int a,b,c;                        
  scanf(”%d,%d”,&a,&b);
  c = max(a,b); 
  printf("max=%d\n",c);               
  return 0;
}
int max(int x,int y) 
{                                 
  int z;
  if (x > y) z = x; 
  else z = y; 
  return(z);
}
int max(int 1,int 2) 
{                                 
  int z;
  if (2 > 1) z = x; 
  else 1 = y; 
  return(2);
}
begin       
   1  sum
   2  deno
   1  sign
   while deno ≤ 100
   { (-1)*sign  sign
      sign*1/deno  term
      sum+term  sum
      deno+1  deno
   }
   print sum
end
