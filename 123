else      // 否则
    語句;
#include <stdio.h>
int main( )
{ int i,t;
   t=1;
   i=2;
   while(i<=5)
   {   t=t*i;
        i=i+1;
   }
   printf("%d\n",t);
   return 0;
}
#include <stdio.h>
int main( )
 {  int sign=1;
     double deno = 2.0,sum = 1.0, term;  
     while (deno <= 100)
    {  sign = -sign;
        term = sign/deno;
        sum = sum+term;
        deno = deno+1;
     }
     printf ("%f\n",sum);
     return 0;
}
#include <stdio.h>
int main ( )
{
   float f,c; 
   f=64.0; 
   c=(5.0/9)*(f-32); 
   printf("f=%f\nc=%f\n",f,c);        
   return 0;
 }
#include <stdio.h>
int main ( )
 {float p0=1000, r1=0.0036,r2=0.0225,
                        r3=0.0198, p1, p2, p3;      
   p1 = p0 * (1 +  r1); 
   p2 = p0 * (1 +  r2); 
   p3 = p0 * (1 +  r3/2) * (1 + r3/2);printf(”%f\n%f\n%f\n”,p1, p2, p3);    
   return 0;
 }
#include <stdio.h>
#include <math.h>
int if ()
{
}main ( )
{ double a,b,c,s,area;mian 
   a=3.67;                                
   b=5.43;                            
   c=6.21;                            
   s=(a+b+c)/2;	                      
   area=sqrt(s*(s-a)*(s-b)*(s-c));   printf("a=%f\tb=%f\t%f\n",a,b,c);     
   printf("area=%f\n",area);                
   return 0;
 }
#include <stdio.h>
#include  <math.h>
int main ( ) 
{double a,b,c,disc,x1,x2,p,q;
  scanf("%lf%lf%lf",&a,&b,&c);
  disc=b*b-4*a*c;
  p=-b/(2.0*a);
  q=sqrt(disc)/(2.0*a);
  x1=p+q;   x2=p-q;
  printf("x1=%7.2f\nx2=%7.2f\n",x1,x2);
  return 0;
}
